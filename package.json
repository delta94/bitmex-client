{
  "name": "bitmex-client",
  "desciption": "A lightweight client for trading on Bitmex exchange.",
  "version": "0.0.0",
  "scripts": {
    "build": "yarn run clean && webpack --mode production",
    "clean": "rimraf dist",
    "dep": "npm-check",
    "lint": "concurrently \"yarn run lint:ts\" \"yarn run lint:scss\"",
    "lint:scss": "stylelint \"src/**/*.scss\"",
    "lint:ts": "tslint \"./**/*.ts\" \"./**/*.tsx\"",
    "start": "concurrently \"ts-node ./secure_proxy.ts\" \"webpack-dev-server --open --config ./webpack.config.ts\"",
    "test": "jest --config ./.jest.json",
    "test:coverage": "jest --coverage --config ./.jest.json",
    "test:watch": "jest --watch --config ./.jest.json"
  },
  "private": true,
  "license": "MIT",
  "author": "Alexander Shelepenok <alxshelepenok@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alxshelepenok/bitmex-client.git"
  },
  "keywords": [
    "bitmex",
    "client"
  ],
  "bugs": {
    "url": "https://github.com/alxshelepenok/bitmex-client/issues"
  },
  "homepage": "https://github.com/alxshelepenok/bitmex-client",
  "dependencies": {
    "@blueprintjs/core": "3.20.0",
    "@blueprintjs/icons": "3.14.0",
    "@blueprintjs/select": "3.12.0",
    "axios": "0.19.2",
    "body-parser": "1.19.0",
    "classnames": "2.2.6",
    "dotenv": "8.1.0",
    "express": "4.17.1",
    "http-proxy": "1.18.1",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-helmet": "5.2.1",
    "react-router-dom": "5.1.2"
  },
  "devDependencies": {
    "@types/autoprefixer": "9.7.2",
    "@types/body-parser": "1.19.0",
    "@types/dotenv": "6.1.1",
    "@types/express": "4.17.7",
    "@types/html-webpack-plugin": "3.2.3",
    "@types/http-proxy": "1.17.4",
    "@types/jest": "25.2.3",
    "@types/node": "12.12.68",
    "@types/react": "16.9.35",
    "@types/react-dom": "16.9.8",
